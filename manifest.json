{
  "manifest_version": 2,

  "name": "Large keyboard size for Google Input Tools",
  "short_name": "Big keyboard",
  "description": "Large font size for Google Input Tools' keyboard.",
  "version": "0.0.3",
  "icons": {
    "240": "thirdparty/icon.png"
  },

  "browser_action": {
    "default_icon": "thirdparty/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "match_about_blank": true,
      "all_frames": true,
      "css": ["style.css"]
    }
  ]
}
